<record name="IBANRule" version="1.0"
        xmlns="http://www.teherba.org/2006/xtrans/Record"
        package="org.teherba.checkdig.account" type="Bean" code="ASCII" length="18"
        rem="Bank-specific mapping rules for German IBAN computation"
        >
    <field name="orid"      rem="original rule id               "  type="String"     />
    <field name="okey"      rem="original key schema            "  type="String"     />
    <field name="oblz"      rem="original BLZ                   "  type="String"     />
    <field name="oacct"     rem="original account no            "  type="String"     />
    <field name="ocheck"    rem="original checking of account no"  type="String"     />
    <field name="nblz"      rem="next BLZ                       "  type="String"     />
    <field name="nacct"     rem="next account no                "  type="String"     />
    <field name="nbic"      rem="next BIC                       "  type="String"     />
    <field name="niban"     rem="next IBAN                      "  type="String"     />
    <field name="nrid"      rem="next rule id                   "  type="String"     />
    <field name="nkey"      rem="next key schema                "  type="String"     />
</record>
