<record name="IBANDetail" version="1.0"
        xmlns="http://www.teherba.org/2006/xtrans/Record"
        package="org.teherba.checkdig.account" type="Bean" code="ASCII" length="18"
        rem="Country specific data for checking of IBAN subfields"
        sql="true" >
    <field name="cc"        rem="Country Code"              type="String" key="1" />
    <field name="width"     rem="Total Length"              type="int"     />
    <field name="bbidStart" rem="Start of Bank/Branch Id"   type="int"     />
    <field name="bbidLen"   rem="Length of Bank/Branch Id"  type="int"     />
    <field name="acctStart" rem="Start of Account Number"   type="int"     />
    <field name="acctLen"   rem="Length of Account Number"  type="int"     />
    <field name="chkMethod" rem="Number of Check Method"    type="int"     />
    <field name="chkStart"  rem="Start of Check Digits"     type="int"     />
    <field name="chkLen"    rem="Length of Check Digits"    type="int"     />
</record>
